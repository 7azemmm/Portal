@model Student




<form method="post" action="/Student/AddStud">

  <div class="form-group">
    <label for="Name">Name</label>
    <input type="text" class="form-control" id="Name" name="Name" placeholder="Name">
    
  </div>

 

  <div class="form-check">
    <label for="Stud_Phone_number"> Phone number</label>
    <input type="text" class="form-control" id="Stud_Phone_number" name="Stud_Phone_number">
  </div>

   <div class="form-check">
    <label for="Stud_Parent_Phone_number"> parent phone number</label>
    <input type="text" class="form-control" id="Stud_Parent_Phone_number" name="Stud_Parent_Phone_number">
  </div>

  <div class="form-group">
            <select class="form-control" name="groupId" id="GroupId">
                @foreach (Group group in ViewBag.groups)
                {
                    <option value="@group.Id">@group.Name</option>
                }
            </select>
    </div>

 
   

  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

